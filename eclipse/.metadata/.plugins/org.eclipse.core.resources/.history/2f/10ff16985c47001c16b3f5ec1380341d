int main() {

	volatile unsigned int* LED_PIO = (unsigned int*)0x1e0;
	volatile unsigned int* SW_PIO = (unsigned int*)0x1c0;
	volatile unsigned int* KEY_PIO = (unsigned int*)0x1b0;
	volatile unsigned int* HEX_PIO = (unsigned int*)0x160;
	*LED_PIO = 0x0;
	while (1) {
		if (!(*KEY_PIO & 1)) {
			*LED_PIO = *SW_PIO;
			while (!(*KEY_PIO & 1));
		}
		if (!(*KEY_PIO & 2)) {
			*LED_PIO -= 1;
			while (!(*KEY_PIO & 2));
		}
		*HEX_PIO = *LED_PIO;
	}
	return 1;
}
