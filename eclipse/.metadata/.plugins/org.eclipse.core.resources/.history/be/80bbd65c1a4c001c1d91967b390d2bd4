#include "stdio.h"
#include "system.h"

#ifndef	RAM_ADDR_BASE
#define RAM_ADDR_BASE 0x1b0
#endif

#ifndef	RAM_DIN_BASE
#define RAM_DIN_BASE 0x1d0
#endif

#ifndef	RAM_DOUT_BASE
#define RAM_DOUT_BASE 0x1c0
#endif

#ifndef	RAM_READ_BASE
#define RAM_READ_BASE 0x1a0
#endif

int main() {
	volatile unsigned int* LED_PIO = LED_BASE;
		volatile unsigned int* SW_PIO = SW_BASE;
		volatile unsigned int* KEY_PIO = KEY_BASE;
		volatile unsigned int* HEX_PIO = HEX_BASE;

		volatile unsigned int* RAM_ADDR = RAM_ADDR_BASE;
		volatile unsigned int* RAM_DIN = RAM_DIN_BASE;
		volatile unsigned int* RAM_DOUT = RAM_DOUT_BASE;
		volatile unsigned int* RAM_READ = RAM_READ_BASE;

		while (1) {
			printf("%x %x %x\n", *RAM_ADDR, *RAM_DIN, *RAM_DOUT);
		}
		return 1;
}
